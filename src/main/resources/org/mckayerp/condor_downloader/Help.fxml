<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>
<SplitPane dividerPositions="0.25" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.mckayerp.condor_downloader.HelpDialogController">
    <ScrollPane hbarPolicy="NEVER" prefHeight="500.0" prefWidth="200.0">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
            <TreeView id="contentsTree" fx:id="helpContent" prefHeight="472.0" prefWidth="198.0"
                      AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
    </ScrollPane>
    <WebView id="helpViewer" fx:id="helpViewer" prefHeight="500.0"/>
</SplitPane>
